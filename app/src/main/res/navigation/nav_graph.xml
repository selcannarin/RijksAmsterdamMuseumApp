<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
xmlns:app="http://schemas.android.com/apk/res-auto"
xmlns:tools="http://schemas.android.com/tools"
android:id="@+id/nav_graph"
app:startDestination="@id/homeFragment">

<fragment
    android:id="@+id/detailFragment"
    android:name="com.selcannarin.rijksamsterdammuseumapp.DetailFragment"
    android:label="fragment_detail"
    tools:layout="@layout/fragment_detail" />
<fragment
    android:id="@+id/homeFragment"
    android:name="com.selcannarin.rijksamsterdammuseumapp.HomeFragment"
    android:label="fragment_home"
    tools:layout="@layout/fragment_home">
    <action
        android:id="@+id/homeToDetail"
        app:destination="@id/detailFragment">
        <argument
            android:name="artObject"
            app:argType="com.selcannarin.rijksmuseumamsterdamcatalog.data.model.ArtObject" />
    </action>

</fragment>
</navigation>
